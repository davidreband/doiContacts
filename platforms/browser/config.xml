<?xml version='1.0' encoding='utf-8'?>
<widget id="org.doichain.contacts.app" version="1.0.49  " xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>DoiContacts</name>
    <description>
       Request qualified email permissions via Doichain blockchain
    </description>
    <author email="nico@le-space.de" href="https://le-space.de">
        Doichain Core Team
    </author>
    <icon src="res/icon.png" />
    <content src="index.html" />
    <access allows-arbitrary-loads-in-media="true" allows-arbitrary-loads-in-web-content="true" allows-local-networking="true" origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <hook src="hooks/020_build_version.js" type="before_build" />
</widget>
